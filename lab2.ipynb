    "import networkx as nx\n",
    "\n",
    "edge = [(0, 1), (0, 2), (0, 3)]\n",
    "G = nx.Graph(edge)\n",
    "\n",
    "centrality = nx.eigenvector_centrality_numpy(G)\n",
    "for n in centrality:\n",
    "    print(f\"c({n})= {centrality[n]:4f}\")"
   ]
